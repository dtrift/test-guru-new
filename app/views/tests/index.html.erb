<h1>Tests</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Level</th>
    <th>Author</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.id %></td>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= test.author.name %></td>
    </tr>
  <% end %>
</table>
